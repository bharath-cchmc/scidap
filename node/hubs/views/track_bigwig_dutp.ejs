<%var orig=data.uid+'_multiwig'; data.parent=orig;
%>track <%= orig %>
container multiWig
aggregate transparentOverlay
showSubtrackColorOnUi on
autoScale on
alwaysZero on
shortLabel <%= data.name4browser %>
longLabel <%= data.name4browser %>
type bigWig

<%
var n4b=data.name4browser;
var d1=data; d1.uidf=d1.uid+'.P';d1.uidt=d1.uid+'_P';d1.name4browser=n4b+' P';%><%-
include('track_bigwig', {data:d1}) %>
<%
var d2=data; d2.uidf=d2.uid+'.R';d2.uidt=d2.uid+'_R';d2.name4browser=n4b+' R';d2.negate=true;d2.color='70,70,245';%><%-
include('track_bigwig', {data:d2}) %>